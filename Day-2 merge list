n=int(input("enter the number of elements: "))
list1=[]
for i in range(n):
    a=input("enter the number to add on list: ")
    list1.append(a)
m=int(input("enter the number of elements : "))
list2=[]
list3=[]
for i in range(m):
    b=input("enter the number to add on list : ")
    list2.append(b)
print(list1)
print(list2)
list3=(list1+list2)
list3.sort()
print("the merge of two list is ",list3)
